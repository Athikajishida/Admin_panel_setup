<h1>Edit User</h1>
<%= form_with model: @user, url: admin_path(@user), method: :patch do |f| %>
  <div>
    <%= f.label :email %>
    <%= f.text_field :email %>
  </div>
  <div>
    <%= f.label :admin %>
    <%= f.check_box :admin %>
  </div>
  <div>
    <%= f.submit "Update User" %>
  </div>
<% end %>
<%= link_to "Back", admin_path(@user) %>
